.coupon,
.use,
.usable,
.grant,
.support,
.receive{

    background:rgba(255,255,255,1);
    margin-top:10rpx;
    padding: 0 29rpx;
     font-size:26rpx;
}
.coupon{
    height:204rpx;
    display: flex;
    flex-direction: column;
}
.coupon-name{
    height: 90rpx;
    line-height: 90rpx;
    border-bottom:1rpx solid #f5f5f5;
}
.coupon-name,
.coupon-denomination{
    font-family:PingFang-SC-Medium;
    font-weight:500;
    display: flex;
}
.coupon-name input{
    margin:20rpx 78rpx;
}
.symbol{
    color: #ff0000;
    line-height: 90rpx;
    margin-right: 20rpx;
    font-size: 30rpx;
}
.coupon-denomination{
    height: 113rpx;
    line-height: 113rpx;
}
.coupon-denomination input{
    width: 200rpx;
    margin-left: 156rpx;
    margin-top: 35rpx;
}
.coupon-denomination-icon{
    background-color: #ffffff;
    margin-top: auto;
    margin-bottom: auto;
    margin-left: 160rpx;
}
.coupon-denomination-icon::after{
    border: none;
}
.use{
    height: 173rpx;
}
.radio1{
    height: 85rpx;
    border-bottom:1rpx solid #f5f5f5;
}
.radio1 label{
    margin-left: 102rpx;
    color: #999999;
}
.radio2{
    height: 86rpx;
    margin-left: 240rpx;
    line-height: 86rpx;
    color: #999999;
    display: flex;
}
.radio2 input{
    width:108rpx;
    height:56rpx;
    background:rgba(245,245,245,1);
    border:1rpx solid rgba(221,221,221,1);
    border-radius:5rpx;
    display: inline-block;
    margin: 18rpx 14rpx;
    padding-left: 10rpx;
}
.usable{
    height: 227rpx;
}
.usable-time,
.other-rule{
    font-size: 22rpx;
    color:rgba(153,153,153,1);
    height: 50rpx;
    line-height: 64rpx;
    margin-left: 22rpx;
}
.usable-form{
    height: 92rpx;
    border-bottom:0.5rpx solid #f5f5f5;
}
.usable-radio{
    display: inline-block;
    margin-left: 155rpx;
}
.usable-day{
    height: 84rpx;
    margin-left: 264rpx;
    line-height: 84rpx;
    color:#333333;
    display: flex;
}
.usable-day input{
    width:108rpx;
    height:56rpx;
    background:rgba(245,245,245,1);
    border:1rpx solid rgba(221,221,221,1);
    border-radius:5rpx;
    display: inline-block;
    margin: 14rpx 14rpx;
    padding-left: 10rpx;
}
.tab{
    margin-left: 50rpx;
    color: #999999;
}
.other{
    height: 134rpx;
    display: flex;
    font-size: 26rpx;
    flex-direction: column;
    margin-top: 10rpx;
    padding: 0 29rpx;
    background:rgba(255,255,255,1);
}
.other-rule{
    height: 50rpx;
}
.everyone{
    height: 84rpx;
    display: flex;
    line-height: 84rpx;
}
.everyone input{
    width:108rpx;
    height:56rpx;
    background:rgba(245,245,245,1);
    border:1rpx solid rgba(221,221,221,1);
    border-radius:5rpx;
    display: inline-block;
    padding-left: 10rpx;
    margin: 14rpx 29rpx 14rpx 220rpx;
}
.grant{
    height: 104rpx;
    display: flex;
    line-height: 104rpx;
}
.grant input{
   display: inline-block;
   margin: auto;
}
.support{
    height: 177rpx;
}
.support-scene{
    height: 85rpx;
    border-bottom:1rpx solid #f5f5f5;
    display: flex;
    line-height: 85rpx;
}
.support-scene radio-group{
    margin-left: 107rpx;
}
.support-goods-shelves{
    height: 90rpx;
    display: flex;
    line-height: 90rpx;
}
.support-goods-shelves input{
      margin:25rpx 0 25rpx 100rpx;
}
.tab1{
    color: #999999;
    float: right;
}
.receive{
    height: 161rpx;
    display: flex;
}
.receive-setup{
    width: 80%;
}
.receive-setup1{
    height: 50%;
    line-height: 120rpx;
    font-size:22rpx;
    font-family:PingFang-SC-Medium;
    font-weight:500;
    color:rgba(255,0,0,1);
}
.body-view{
    margin: auto;
}
.preserve{
    height: 186rpx;
}
.preserve-bt{
  width:690rpx;
  height:80rpx;
  background:linear-gradient(132deg,rgba(26,165,145,1),rgba(29,199,174,1));
  box-shadow:0rpx 3rpx 5rpx 0rpx rgba(0, 0, 0, 0.1);
  border-radius:20rpx;
  font-size:26rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height: 80rpx;
  margin-top: 57rpx;
}
.preserve-bt-hover{
    background: rgb(25, 138, 121)
}
.shelf{
    height: 128rpx;
    background-color: #ffffff;
    display: flex;
    padding: 0 30rpx;
    font-size: 26rpx;
    margin-top:10rpx;
}
.shelfName text{
    display: block;
}
.shelfName{
    width: 20%;
}
.shelf-tab{
    width: 5%;
    height: 128rpx;
    line-height: 128rpx;
    margin-left: 75%;
}
.stock-details{
    height: 200rpx;
    background-color: #ffffff;
    padding: 0 30rpx;
    font-size: 26rpx;
    margin-top:20rpx;
}
.bor-bom{
    height: 128rpx;
    border-bottom: 1rpx solid #f5f5f5;
    border-top: 1rpx solid #f5f5f5;
    display: flex;
}
.shelfName-1{
    margin:20rpx 0;   
}
.stock-information{
    width: 40%;
    margin-left: 40%;
    text-align: right;
}
.money{
    display: flex;
    height: 70rpx;
    line-height: 70rpx;
}
.money-1{
    width: 20%;
}
.money-number{
    width: 20%;
    margin-left: 60%;
    text-align: right;
}
.preserve{
    height: 120rpx;  
}
.preserve-bt{
  width:690rpx;
  height:80rpx;
  background:linear-gradient(132deg,rgba(26,165,145,1),rgba(29,199,174,1));
  box-shadow:0rpx 3rpx 5rpx 0rpx rgba(0, 0, 0, 0.1);
  border-radius:20rpx;
  font-size:26rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height: 80rpx;
  margin:20rpx auto;
}
.preserve-bt-hover{
    background: rgb(25, 138, 121)
}
.by{
    height: 930rpx;
    width: 100%;
}
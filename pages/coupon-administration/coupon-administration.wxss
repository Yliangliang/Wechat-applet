
.swiper-tab{  
  width: 100%;
  height:100rpx;
  text-align: center;  
  line-height: 80rpx;
  border-bottom: 1rpx solid #999999;
  box-shadow: 0rpx 0rpx 10rpx 5rpx #aaa;
  background-color: #ffffff;
  position: fixed;
  top: 0rpx;
  z-index: 9999;
}  
.swiper-tab-list{  
  font-size: 26rpx;  
  display: inline-block;  
  width: 33.33%;  
  color: #999999;  
}  
.on{
  color: #1AA591;  
}
.swiper-tab-1{  
  width: 0rpx;
  height: 0rpx;
  margin:0rpx auto;
}
.in{
  width: 40rpx;  
  border-bottom: 6rpx solid #1AA591;
}
.swiper-box{ 
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
  top: 100rpx;
}
.swiper-box view{  
  text-align: center;  
}
.add-coupon{
  width:690rpx;
  height:80rpx;
  background:linear-gradient(132deg,rgba(26,165,145,1),rgba(29,199,174,1));
  box-shadow:0rpx 3rpx 5rpx 0rpx rgba(0, 0, 0, 0.1);
  border-radius:20rpx;
  font-size:26rpx;
  font-family:PingFang-SC-Medium;
  font-weight:500;
  color:rgba(255,255,255,1);
  line-height: 80rpx;
}
.content{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.body{
  height: 100%;
}
.foot{
  height: 80rpx;
  margin-bottom: 50rpx;

}